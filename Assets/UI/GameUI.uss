.healthbar {
    width: 45%;
    margin-top: auto;
    margin-right: auto;
    margin-bottom: auto;
    margin-left: auto;
}

.healthbar .unity-progress-bar__progress {
    background-color: rgb(154, 0, 0);
}

.xpbar .unity-progress-bar__progress {
    background-color: rgb(230, 238, 0);
}

.xpbar .unity-progress-bar__container {
    min-height: 50%;
    max-height: 50%;
}
